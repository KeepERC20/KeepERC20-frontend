{
    "name": "KeeperC",
    "description": "Smart contract wallet, powered by Chainlink Automation",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {},
    "icons": {
        "16": "icons/KeeperC_logo_16.png",
        "32": "icons/KeeperC_logo_32.png",
        "48": "icons/KeeperC_logo_48.png",
        "128": "icons/KeeperC_logo_128.png",
        "192": "icons/KeeperC_logo_192.png",
        "256": "icons/KeeperC_logo_256.png",
        "512": "icons/KeeperC_logo_512.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "KeeperC",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "management",
        "storage"
    ]
}